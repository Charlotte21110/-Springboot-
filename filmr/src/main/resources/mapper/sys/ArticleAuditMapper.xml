<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.design.filmr.sys.mapper.ArticleAuditMapper">
    <select id="getAuditIdByArticleId" resultType="Integer">
        select audit_id from article_audit
        where article_id = #{article_id}
    </select>
</mapper>
