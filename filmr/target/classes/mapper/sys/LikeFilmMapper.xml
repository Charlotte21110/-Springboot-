<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.design.filmr.sys.mapper.LikeFilmMapper">
    <select id="getLikeIdByFilmId" resultType="Integer">
        select like_id from like_film
        where film_id = #{film_id}
    </select>
</mapper>
